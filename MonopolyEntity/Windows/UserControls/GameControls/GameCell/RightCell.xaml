<UserControl x:Class="MonopolyEntity.Windows.UserControls.GameControls.GameCell.RightCell"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MonopolyEntity.Windows.UserControls.GameControls.GameCell"
             mc:Ignorable="d" 
             d:DesignHeight="65" d:DesignWidth="150">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="5*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        
        <Canvas Grid.Column="1" x:Name="MoneyPlacer" Background="Gray">

        </Canvas>

        <Grid Grid.Column="0" Name="ImagePlacer" 
              Background="White">

        </Grid>

        <Canvas Grid.Column="0" Name="ChipsPlacer" Panel.ZIndex="100">

        </Canvas>

    </Grid>
</UserControl>
